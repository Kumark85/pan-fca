---
vnet_network:
  name: operations 
  peers:
    - VPCTransit
  network: "10.217.128.0/24"
  subnet:
  - name: Management
    network: "10.217.128.64/27"
    type: trusted
    rte: Default_RT
  - name: Trust
    network: "10.217.128.32/27"
    type: trusted
    rte: internal_network
  - name: Untrust
    type: untrusted
    network: "10.217.128.0/27"
    rte: default_internet
  - name: EgressLB
    type: untrusted
    network: "10.217.128.96/27"
    rte: default_internet

route_tables:
  - name: spoke_rt
    routes:
      - cidr: "10.217.128.0/24"
        gateway: "10.217.128.29"
        name: spoke_to_transit

security_groups:
  - name: Outbound allow
    priority: 100
    direction: inbound
    action: allow
    src_ip: 0.0.0.0
    src_port: any
    dst_port: any
    dst_network: 0.0.0.0.
    protocol: any

tags:
  - name: "environment"
    value: "dev"
  - name: "costcenter"
    value: "it"


